{
  "version": "1.0.2",
  "keywords": [
    "webtools",
    "link",
    "custom",
    "field",
    "plugin",
    "strapi"
  ],
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json"
  },
  "dependencies": {
    "@strapi/design-system": "^2.0.0-rc.29",
    "@strapi/icons": "^2.0.0-rc.29",
    "react-intl": "^7.1.11"
  },
  "devDependencies": {
    "@strapi/strapi": "^5.23.0",
    "@strapi/sdk-plugin": "^5.3.2",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@strapi/typescript-utils": "^5.23.0",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "@strapi/strapi": "^5.23.0",
    "@strapi/sdk-plugin": "^5.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19"
  },
  "strapi": {
    "kind": "plugin",
    "name": "strapi-plugin-link-custom-field",
    "displayName": "Link Custom Field",
    "description": "Grab Slugs and url paths from other content types "
  },
  "name": "strapi-plugin-link-custom-field",
  "description": "Grab Slugs and url paths from other content types ",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/d1sxcat/strapi-plugin-link-custom-field.git"
  },
  "bugs": {
    "url": "https://github.com/d1sxcat/strapi-plugin-link-custom-field/issues"
  },
  "homepage": "https://github.com/d1sxcat/strapi-plugin-link-custom-field#readme",
  "author": "d1sxcat <shaun.mills@live.com>"
}
